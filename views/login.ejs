<%- include('partials/header')%>

<div id="container">
    <div class="form-wrap">
        <h2>Login</h2>
        <form method="POST">
            <div class="form-group">
                <label for="username">User Name</label>
                <input type="text" name="username" id="username" required />
            </div>
            <div class="form-group">
                <label for="password">Password</label>
                <input
                    type="password"
                    name="password"
                    id="password"
                    required
                />
            </div>
            <button
                class="btn"
            >
                Log In
            </button>
        </form>
    </div>
</div>

<%- include('partials/footer')%>